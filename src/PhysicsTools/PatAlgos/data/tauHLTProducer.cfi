module HLT1Tau = PATHLTProducer {
    InputTag triggerResults  = TriggerResults::HLT
    string   triggerName     = "HLT1Tau"
    InputTag filterName      = filterL3SingleTau::HLT
}

module HLT2TauPixel = PATHLTProducer {
    InputTag triggerResults  = TriggerResults::HLT
    string   triggerName     = "HLT2TauPixel"
    InputTag filterName      = filterL25PixelTau::HLT
}

sequence tauHLTProducer = {
    HLT1Tau,
    HLT2TauPixel
}
